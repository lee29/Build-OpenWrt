
config samba
	option workgroup 'WORKGROUP'
	option homes '1'
	option autoshare '1'
	option name 'B70'
	option description 'B70'

config sambashare
	option browseable 'yes'
	option name 'B70'
	option path '/mnt/sda'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0777'
	option dir_mask '0777'
